{
  "name": "@stark-tech-space/api-package-template",
  "version": "1.0.0",
  "description": "API package template",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run build",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stark-tech-space/api-package-template.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@stark-tech-space"
  },
  "keywords": [],
  "author": "@stark-tech-space",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stark-tech-space/api-package-template/issues"
  },
  "homepage": "https://github.com/stark-tech-space/api-package-template#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "dotenv": "^9.0.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "jest": {
    "testTimeout": 30000,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
